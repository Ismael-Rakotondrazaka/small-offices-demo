{
  "name": "nuxt-fusion",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "build:max": "node --max-old-space-size=12288 node_modules/nuxt/bin/nuxt.mjs build",
    "dev": "nuxt dev",
    "dev:max": "node --max-old-space-size=12288 node_modules/nuxt/bin/nuxt.mjs dev",
    "dev:host": "nuxt dev --host",
    "dev:host:max": "node --max-old-space-size=12288 node_modules/nuxt/bin/nuxt.mjs dev --host",
