    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "typecheck": "nuxt typecheck",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "db:generate": "dotenv -- pnpm prisma generate",
    "db:push": "dotenv -- pnpm prisma db push",
    "db:reset": "dotenv -- pnpm prisma migrate reset",
    "db:seed": "dotenv -- pnpm tsx server/services/prisma/seeders/index.ts",
    "prepare": "nuxt prepare",
    "test": "vitest --run",
    "test:coverage": "vitest --run --coverage",
    "test:app": "vitest --run --project app",
    "test:server": "vitest --run --project server",
    "test:shared": "vitest --run --project shared",
    "test:server-core": "vitest --run --project server-core",
    "test:server-api": "vitest --run --project server-api",
    "test:watch": "vitest",
    "test:ui": "vitest --ui --open"
